  width: 100%;
  height: 100%;
  background: #fff;
  padding: 0 1em;
}
.reviewFullPopup header {
  border-bottom: none;
  line-height: 1;
  position: relative;
  color: #414042;
  font-weight: bold;
  margin-bottom: 20px;
